NameVirtualHost 127.0.0.1:80

<Directory "%basedir%">
    AllowOverride All
</Directory>

<VirtualHost 127.0.0.1:80>
    ServerName   "one.test.app"
    ServerAlias  "one.test.app"
    DocumentRoot "%basedir%/one"
    DirectoryIndex index.php index.html index.html.var
</VirtualHost>

<VirtualHost 127.0.0.1:80>
    ServerName   "two.test.app"
    ServerAlias  "two.test.app"
    DocumentRoot "%basedir%/two"
    DirectoryIndex index.php index.html index.html.var
</VirtualHost>
